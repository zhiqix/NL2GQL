# this is the schema of this graph
# Nodes
class Tag():
    def __init__(self,tag_name):
        self.tag_name=tag_name

class player(Tag):
    def __init__(self,vid,name:str,age:int):
        self.vid=vid
        self.name=name
        self.age=age

class team(Tag):
    def __init__(self,vid,name:str):
        self.vid=vid
        self.name=name


# Edge
class Edge():
    def __init__(self,edge_type_name):
        self.edge_type_name=edge_type_name

class follow(Edge):
    def __init__(self,src_vid,dst_vid,degree:int):
        self.src_vid=src_vid
        self.dst_vid=dst_vid
        self.degree=degree

class serve(Edge):
    def __init__(self,src_vid,dst_vid,start_year:int,end_year:int):
        self.src_vid=src_vid
        self.dst_vid=dst_vid
        self.start_year=start_year
        self.end_year = end_year